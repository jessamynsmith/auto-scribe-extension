{
  "manifest_version": 2,
  "name": "Image to Text",
  "description": "Extract text from images.",
  "version": "1",
  "author": "Jessamyn Smith",
  "browser_action": {
    "default_title": "Image to Text"
  },
  "content_scripts": [
    {
      "css": [
        "overlay.css"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "<all_urls>"
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "48": "favicon-48x48.png"
  },
  "background": {
    "page": "background.html"
  }
}
